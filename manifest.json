{
  "manifest_version": 2,
  "name": "Jerome Extension",
  "description": "Jerome's first extension",
  "version": "1.0",
  "icons": {
    "16": "/images/MH16.png",
    "48": "/images/MH48.png",
    "128": "/images/MH128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "/images/MH16.png",
      "48": "/images/MH48.png",
      "128": "/images/MH128.png"
    }
  },
  "content_scripts": [{
    "matches": ["https://www.google.com/*"],
    "css": ["main.css"]
  }]
}